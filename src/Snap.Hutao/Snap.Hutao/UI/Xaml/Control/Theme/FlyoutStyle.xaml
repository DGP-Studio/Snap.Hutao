<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style
        x:Key="WebViewerFlyoutPresenterStyle"
        BasedOn="{StaticResource DefaultFlyoutPresenterStyle}"
        TargetType="FlyoutPresenter">
        <Setter Property="BorderBrush" Value="{ThemeResource CardStrokeColorDefaultBrush}"/>
        <Setter Property="Padding" Value="0"/>
    </Style>
    <Style
        x:Key="FlyoutPresenterPadding0Style"
        BasedOn="{StaticResource DefaultFlyoutPresenterStyle}"
        TargetType="FlyoutPresenter">
        <Setter Property="Padding" Value="0"/>
    </Style>
    <Style
        x:Key="FlyoutPresenterPadding0And2Style"
        BasedOn="{StaticResource DefaultFlyoutPresenterStyle}"
        TargetType="FlyoutPresenter">
        <Setter Property="Padding" Value="0,2"/>
    </Style>
    <Style
        x:Key="FlyoutPresenterPadding2Style"
        BasedOn="{StaticResource DefaultFlyoutPresenterStyle}"
        TargetType="FlyoutPresenter">
        <Setter Property="Padding" Value="2"/>
    </Style>
    <Style
        x:Key="FlyoutPresenterPadding4Style"
        BasedOn="{StaticResource DefaultFlyoutPresenterStyle}"
        TargetType="FlyoutPresenter">
        <Setter Property="Padding" Value="4"/>
    </Style>
    <Style
        x:Key="FlyoutPresenterPadding6Style"
        BasedOn="{StaticResource DefaultFlyoutPresenterStyle}"
        TargetType="FlyoutPresenter">
        <Setter Property="Padding" Value="6"/>
    </Style>
    <Style
        x:Key="FlyoutPresenterPadding8Style"
        BasedOn="{StaticResource DefaultFlyoutPresenterStyle}"
        TargetType="FlyoutPresenter">
        <Setter Property="Padding" Value="8"/>
    </Style>
    <Style
        x:Key="FlyoutPresenterPadding12Style"
        BasedOn="{StaticResource DefaultFlyoutPresenterStyle}"
        TargetType="FlyoutPresenter">
        <Setter Property="Padding" Value="12"/>
    </Style>
    <Style
        x:Key="FlyoutPresenterPadding16And10Style"
        BasedOn="{StaticResource DefaultFlyoutPresenterStyle}"
        TargetType="FlyoutPresenter">
        <Setter Property="Padding" Value="16,10"/>
    </Style>
</ResourceDictionary>
